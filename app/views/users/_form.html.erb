<%= form_with(model: user, local: true) do |form| %>

  <div class="field">
  <%= form.label :name %>
  <%= form.text_field :name, placeholder: "Full Name" %>
  </div>

  <div class="field">
  <%= form.label :phone %>
  <%= form.text_field :phone, placeholder: "Phone Number" %>
  </div>

  <%= link_to "Submit", user_path %>
<% end %>
